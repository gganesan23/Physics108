    "import numpy as np\n",
    "import pyvisa as visa\n",
    "\n",
    "def inithp(instr):\n",
    "    rm = visa.ResourceManager()\n",
    "    hp = rm.open_resource(instr)\n",
    "    hp.write('VSET 12')\n",
    "    return hp\n",
    "\n",
    "# read X and phase from srs\n",
    "def readhp(srs):\n",
    "    return srs.query_ascii_values('SNAP?1,4')"
   ]
